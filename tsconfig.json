{
  "compilerOptions": {
    "jsx": "react",
    "noEmit": true,
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "importHelpers": true,
    "moduleResolution": "node",
    "removeComments": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es5",
    "baseUrl": ".",
    "lib": [
      "dom",
      "es2015.collection", 
      "es2015.core",
      "es2015.promise",
      "es2015.iterable",
      "es2015.symbol",
      "es2016.array.include",
      "es5"
    ]
  }
}
