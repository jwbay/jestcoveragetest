{
	"coverageReporters": [
		"text-summary",
		"html"
	],
	"collectCoverage": true,
	"collectCoverageFrom": [
		"**/*.{ts,tsx}"
	],
	"moduleFileExtensions": [
		"ts",
		"tsx",
		"js",
		"json"
	],
	"notify": true,
	"transform": {
		".*": "<rootDir>/preprocessor.js"
	},
	"testRegex": "test.tsx?$",
	"unmockedModulePathPatterns": [
		"enzyme",
		"react"
	]
}