{
	"coverageReporters": [
		"text-summary",
		"html"
	],
	"collectCoverage": true,
	"collectCoverageFrom": [
		"src/**/*"
	],
	"mapCoverage": true,
	"moduleFileExtensions": [
		"ts",
		"tsx",
		"js",
		"json",
		"coffee"
	],
	"transform": {
		"typescript": "<rootDir>/transform-typescript.js",
		"babel": "<rootDir>/transform-babel.js",
		"buble": "<rootDir>/transform-buble.js",
		"coffeescript": "<rootDir>/transform-coffeescript.js"
	},
	"testRegex": "src/(.+)\\.test\\."
}
